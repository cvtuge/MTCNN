net : "./det1_train.prototxt"
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.001
momentum: 0.9
weight_decay: 0.001
# The learning rate policy
lr_policy: "step"
stepsize: 50000
gamma: 0.8
display: 100   #500
max_iter: 600000
snapshot: 20000
snapshot_prefix: "./train_model/det1_"
solver_mode: GPU
